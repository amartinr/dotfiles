!~/bin/atomic-tty.sh special
*.foreground:   #eff1f5
*.background:   #2b303b
*.cursorColor:  #eff1f5

! black
*.color0:       #2b303b
*.color8:       #343A47

! red
*.color1:       #733A40
*.color9:       #bf616a

! green
*.color2:       #6E805E
*.color10:      #a3be8c

! yellow
*.color3:       #A68F61
*.color11:      #ebcb8b

! blue
*.color4:       #6F7C8A
*.color12:      #8fa1b3

! magenta
*.color5:       #856980
*.color13:      #b48ead

! cyan
*.color6:       #6099A1
*.color14:      #74B8C2

! white
*.color7:       #DBE0EB
*.color15:      #eff1f5

! URxvt
URxvt*termName: xterm-256color

#define fonts xft:Liberation Mono for Powerline:regular:size=11.2:antialias=true, \
              xft:Ionicons:medium:size=11:antialias=true

URxvt.font: fonts
URxvt.boldFont: xft:Liberation Mono for Powerline:bold:size=11.2:antialias=true
URxvt.letterSpace: -1

URxvt.scrollBar: false
URxvt*loginShell: false
URxvt*internalBorder: 1

! end and home keys
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~

! ctrl + arrow keys
URxvt.keysym.Control-Up:   \033[1;5A
URxvt.keysym.Control-Down:    \033[1;5B
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C

URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show
