! special
*.foreground:      #D6B89A
*.background:      #181512
*.cursorColor:      #bea492

! black
*.color0: #332d29
*.color8: #817267

! red
*.color1: #703F3F
*.color9: #A35C5C

! green
*.color2: #607051
*.color10: #8CA376

! yellow
